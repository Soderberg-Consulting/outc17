<?xml version="1.0" encoding="utf-8"?>
<?pcf-stylesheet path="/outc17/xsl/data-file.xsl" title="Data File" extension="html"?>

<!DOCTYPE document SYSTEM "http://commons.omniupdate.com/dtd/standard.dtd"> 

<document xmlns:ouc="http://omniupdate.com/XSL/Variables">
	<ouc:info><tcf>section.tcf</tcf><tmpl>properties.tmpl</tmpl></ouc:info>

	<page type="library-journal"/>

	<ouc:properties label="config">
		<parameter section="Database Information" name="database-name" type="text" group="Everyone" prompt="Database Name" >A Sample Journal</parameter>
		<parameter name="database-description" type="text" group="Everyone" prompt="Description" >This is a sample journal that can be used as a template!</parameter>
		<parameter name="database-url" type="text" group="Everyone" prompt="URL" >http://www.unco.edu</parameter>

		<parameter section="Active" name="active" type="select" group="Everyone" prompt="Active">
<option value="yes" selected="false">True</option>
<option value="no" selected="true">False</option>
</parameter>

	</ouc:properties>

</document>
